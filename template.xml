<?xml version="1.0" encoding="UTF-8"?>
<Document xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns="urn:iso:std:iso:20022:tech:xsd:pain.001.001.03">
    <CstmrCdtTrfInitn>
        <GrpHdr>
            <MsgId>{{=it.GrpHdr.MsgId}}</MsgId>
            <CreDtTm>{{=it.GrpHdr.CreDtTm}}</CreDtTm>
            <NbOfTxs>{{=it.GrpHdr.NbOfTxs}}</NbOfTxs>
            <CtrlSum>{{=it.GrpHdr.CtrlSum}}</CtrlSum>
            <InitgPty>
                <Nm>{{=it.GrpHdr.InitgPty.Nm}}</Nm>
                <PstlAdr>
                    <StrtNm>{{=it.GrpHdr.InitgPty.PstlAdr.StrtNm}}</StrtNm>
                    <BldgNb>{{=it.GrpHdr.InitgPty.PstlAdr.BldgNb}}</BldgNb>
                    <PstCd>{{=it.GrpHdr.InitgPty.PstlAdr.PstCd}}</PstCd>
                    <TwnNm>{{=it.GrpHdr.InitgPty.PstlAdr.TwnNm}}</TwnNm>
                    <Ctry>{{=it.GrpHdr.InitgPty.PstlAdr.Ctry}}</Ctry>
                </PstlAdr>
            </InitgPty>
        </GrpHdr>
        <PmtInf>
            <PmtInfId>{{=it.PmtInf.PmtInfId}}</PmtInfId>
            <PmtMtd>{{=it.PmtInf.PmtMtd}}</PmtMtd>
            <BtchBookg>{{=it.PmtInf.BtchBookg}}</BtchBookg>
            <ReqdExctnDt>{{=it.PmtInf.ReqdExctnDt}}</ReqdExctnDt>
            <Dbtr>
                <Nm>{{=it.GrpHdr.InitgPty.Nm}}</Nm>
                <PstlAdr>
                    <StrtNm>{{=it.GrpHdr.InitgPty.PstlAdr.StrtNm}}</StrtNm>
                    <BldgNb>{{=it.GrpHdr.InitgPty.PstlAdr.BldgNb}}</BldgNb>
                    <PstCd>{{=it.GrpHdr.InitgPty.PstlAdr.PstCd}}</PstCd>
                    <TwnNm>{{=it.GrpHdr.InitgPty.PstlAdr.TwnNm}}</TwnNm>
                    <Ctry>{{=it.GrpHdr.InitgPty.PstlAdr.Ctry}}</Ctry>
                </PstlAdr>
            </Dbtr>
            <DbtrAcct>
                <Id>
                    <IBAN>{{=it.PmtInf.DbtrAcct.iban}}</IBAN>
                </Id>
            </DbtrAcct>
            <DbtrAgt>
                <FinInstnId>
                    <BIC>{{=it.PmtInf.DbtrAgt.bic}}</BIC>
                </FinInstnId>
            </DbtrAgt>
            {{~it.transactions :transaction}}
            <CdtTrfTxInf>
                <PmtId>
                    <InstrId>{{=transaction.InstrId}}</InstrId>
                    <EndToEndId>{{=transaction.EndToEndId}}</EndToEndId>
                </PmtId>
                <Amt>
                    <InstdAmt Ccy="{{=transaction.Amount.Currency}}">{{=transaction.Amount.Amount}}</InstdAmt>
                </Amt>
                <ChrgBr>{{=transaction.ChrgBr}}</ChrgBr>
                <CdtrAgt>
                    <FinInstnId>
                        <BIC>{{=transaction.CdtrAgt.bic}}</BIC>
                    </FinInstnId>
                </CdtrAgt>
                <Cdtr>
                    <Nm>{{=transaction.Cdtr.Nm}}</Nm>
                    <PstlAdr>
                        <StrtNm>{{=transaction.Cdtr.PstlAdr.StrtNm}}</StrtNm>
                        <BldgNb>{{=transaction.Cdtr.PstlAdr.BldgNb}}</BldgNb>
                        <PstCd>{{=transaction.Cdtr.PstlAdr.PstCd}}</PstCd>
                        <TwnNm>{{=transaction.Cdtr.PstlAdr.TwnNm}}</TwnNm>
                        <Ctry>{{=transaction.Cdtr.PstlAdr.Ctry}}</Ctry>
                    </PstlAdr>
                </Cdtr>
                <CdtrAcct>
                    <Id>
                        <IBAN>{{=transaction.CdtrAcct.iban}}</IBAN>
                    </Id>
                </CdtrAcct>
            </CdtTrfTxInf>
            {{~}}
        </PmtInf>
    </CstmrCdtTrfInitn>
</Document>
